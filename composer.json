  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/plugins/login-mods/": [
        "wpmudev/login-message"
      ],
      "wp-content/plugins/wpmudev-updates/": [
        "wpmudev/wpmu-dev-dashboard"
      ],
      "wp-content/plugins/ultimate-facebook/": [
        "wpmudev/ultimate-facebook"
      ],
      "wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "rm wp/composer.json",
      "rm -rf wp/wp-content/",
      "cp -r wp/* ./",
      "rm -rf wp/"
    ],
    "post-update-cmd": [
      "rm wp/composer.json",
      "rm -rf wp/wp-content/",
      "cp -r wp/* ./",
      "rm -rf wp/"
    ]
  },
  "autoload": {}
